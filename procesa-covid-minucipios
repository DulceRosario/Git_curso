for MUNICIPIO  in $(cat muni2.csv |  cut -d, -f3 | grep -v Nacional | grep -v nombre) 

do  

#echo "Bienvenido al programa de $0 "
echo "procesando el municipio de $MUNICIPIO $(grep -i "$MUNICIPIO" muni2.csv | cut -d, -f1-3 --complement | tr "," "+" | bc) " 

done 
