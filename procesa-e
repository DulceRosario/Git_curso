

# Obtener lista
# cat covid2.csv |  cut -d, -f3 | grep -v nombre | grep -v Nacional 

# Obtiene acumulado
# grep DURANGO covid2.csv | cut -d, -f1-3 --complement | tr "," "+" | bc 


for ESTADO in $(cat covid2.csv |  cut -d, -f3 | grep -v nombre | grep -v Nacional)

do 

echo "$ESTADO $(grep $ESTADO covid2.csv | cut -d, -f1-3 --complement | tr "," "+" | bc )"  

done | sort -k 2 -n 

